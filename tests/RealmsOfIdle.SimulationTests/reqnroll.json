{
  "$schema": "https://reqnroll.net/reqnroll-config.json",
  "language": {
    "feature": "en-US"
  },
  "bindingCulture": {
    "name": "en-US"
  },
  "generator": {
    "allowDebugGeneratedFiles": false,
    "allowRowTests": true
  },
  "runtime": {
    "stopAtFirstError": false,
    "missingOrPendingStepsOutcome": "Ignore",
    "obsoleteBehavior": "None"
  },
  "trace": {
    "traceSuccessfulSteps": true,
    "traceTimings": true,
    "minTracedDuration": "0:0:0.1",
    "stepDefinitionSkeletonStyle": "RegexAttribute",
    "coloredOutput": true
  }
}
